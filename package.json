{
  "name": "allergy-aware-recipe-finder",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start",
    "build:web": "npx expo export --platform web",
    "lint": "npx expo lint",
    "build:ios:native": "rm -rf node_modules/.cache/babel-loader/* .expo ios && npx expo prebuild --platform ios --clean",
    "build:android:native": "rm -rf node_modules/.cache/babel-loader/* .expo android && npx expo prebuild --platform android --clean",
    "build:native": "rm -rf node_modules/.cache/babel-loader/* .expo ios android && npx expo prebuild --clean",
    "start:dev": "npx expo start --dev-client --clear",
    "start-clear": "npx expo start --clear",
    "android": "npx expo run:android",
    "ios:native": "rimraf -rf node_modules/.cache/babel-loader/* && rimraf -rf .expo && npx expo run:ios",
    "ios": "npx expo run:ios",
    "ios:prod": "npx expo run:ios --configuration Release",
    "web": "rimraf -rf node_modules/.cache/babel-loader/* && rimraf -rf .expo && npx expo start -w -c"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo-google-fonts/open-sans": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "axios": "^1.6.2",
    "expo": "53.0.9",
    "expo-blur": "~14.1.4",
    "expo-camera": "~16.1.6",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "rimraf": "^6.0.1",
    "expo-dev-client": "~5.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
